#include<stdio.h>
void Selection_sort(int a[],int n){
    int i,min,j;
    int temp;
    for(i=0;i<n-1;i++){
        min=i;
        for(j=i+1;j<n;j++){
            if(a[j]<a[min]){
                min=j;
            }
        }
        if(min!=i){
            temp=a[i];
            a[i]=a[min];
            a[min]=temp;
        }
    }
}
int main(){
    int a[6]={9,5,1,8,50,10};
    printf("Before Sorted :");
    for(int i=0;i<6;i++){
        printf("%d ",a[i]);
    }
    Selection_sort(a,6);
    printf("\nSorted array :");
    for(int i=0;i<6;i++){
        printf("%d ",a[i]);
    }
    return 0;
}
