#include <stdio.h>
int main(){
   int a[8]={15,5,20,35,3,42,67,17};
   int data;
   printf("Enter the data you want to find:");
   scanf("%d",&data);
   int i,n=8;
   int found=0;
   for(i=0;i<n;i++){
    if(a[i]==data){
        printf("Element found at position:%d",i+1);
        found=1;
        break;
    }
   }
   if (!found) {
       printf("Element not found in the array.\n");
   }
    return 0;
}

