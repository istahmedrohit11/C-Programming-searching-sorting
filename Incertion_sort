#include<stdio.h>
void Incertion_shot(int a[],int n){
    int i,temp,j;
    for(int i=1;i<n;i++){
        temp=a[i];
        j=i-1;
        while(j>=0 && a[j]>temp){
            a[j+1]=a[j];
            j--;
        }
        a[j+1]=temp;
    }
}
int main(){
    int a[6]={9,5,1,8,50,10};
    Incertion_shot(a,6);
    printf("Sorted array is:");
    for(int i=0;i<6;i++){
        printf("%d ",a[i]);
    }
    return 0;
}
